package com.xiaomu.event
{
	import flash.events.Event;
	
	public class DataManagerEvent extends Event
	{

		/**
		 * 团队数据发生变化的时候派发 
		 */		
		public static const TEAM_DATA_CHANGED:String = "teamDataChanged";
		/**
		 * 学生数据发生变化的时候派发 
		 */		
		public static const STUDENT_DATA_CHANGED:String = "studentDataChanged";
		/**
		 * 问题数据发生变化的时候派发 
		 */		
		public static const QUESTION_DATA_CHANGED:String = "questionDataChanged";
		/**
		 * 回答数据发生变化的时候派发 
		 */		
		public static const ANSWER_DATA_CHANGED:String = "answerDataChanged";
		
		public function DataManagerEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
		
	}
}